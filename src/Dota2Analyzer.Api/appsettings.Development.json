{
  "Analyzer": {
    "CacheOnly": false,
    "DisableBenchmarks": false,
    "AvoidExternalWhenCached": true
  },
  "Assets": {
    "HeroImageBaseUrl": "https://cdn.cloudflare.steamstatic.com/apps/dota2/images/heroes",
    "HeroImageFallbackBaseUrl": "https://cdn.opendota.com/apps/dota2/images/heroes",
    "ItemImageBaseUrl": "https://cdn.cloudflare.steamstatic.com",
    "ItemImageFallbackBaseUrl": "https://cdn.opendota.com"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging": "Information",
      "Dota2Analyzer": "Debug",
      "Dota2Analyzer.Api.Services.HeroImageCache": "Debug",
      "Dota2Analyzer.Api.Services.ItemImageCache": "Debug"
    }
  }
}
